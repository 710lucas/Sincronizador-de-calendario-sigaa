var sistema = "/sigaa";


var PainelNoticia = (function() {
	return {
		show : function(idNoticia){        	
        	BootstrapDialog.show({
        		title: 'Not&#237;cia',
        		message: jQuery('<div><i class="fa fa-spinner fa-pulse"></i> Carregando...</div>').load( '/sigaa/portais/viewNoticia.jsf?id='+idNoticia+'&ajaxRequest'),
        		buttons: [
					{
					    label: 'Ver todas as not&#237;cias',
					    action: function(dialog) {
					        dialog.close();
					        var url = sistema + "/portal/noticias/todas/";
					        window.open(url,'','width=690,height=430, top=100, left=100, scrollbars' );
					    }
					},      
        		    {
                    label: 'Fechar',
                    cssClass: 'btn-primary',
                    action: function(dialog) {
                        dialog.close();
                    }
                }],
            });
        }
	};
})();
</script>
