<script>
jQuery(function ($) {
	
	$('table.minhas-turmas').each( function(){
		var table = $(this);

		//ativa os elementos tooltip marcados com tooltip
		table.find('[data-toggle="tooltip"]').tooltip();
		
		//oculta legenda se n&#227;o h&#225; evento do tipo
		if (table.find( 'tbody .label-avaliacao' ).size() == 0 ){ 
			table.find("#legenda-avaliacao").hide(); 
		}
		if (table.find( 'tbody .label-atividade' ).size() == 0 ){ 
			table.find("#legenda-atividade").hide();
		}
		if (table.find( 'tbody .label-noticia' ).size() == 0 ){ 
			table.find("#legenda-noticia").hide();
		}

		//limita o tamanho m&#225;ximo ocupado pelos elementos de notifica&#231;&#227;o
		const NUM_MAX_NOTICICACOES = 4;
		const MAX_WIDTH_LEGENDAS = 210;
		const MIN_WIDTH_LEGENDAS = 65;
		var qtd_notificacoes = 0;
		var width_legendas = MAX_WIDTH_LEGENDAS;
		
		table.find('tbody > tr').each( function(){
			var qtd = $( this ).find("span.label").size();
			//maior qtd de eventos de todas as turmas
			qtd_notificacoes = ( qtd > qtd_notificacoes )? qtd : qtd_notificacoes;
		});
		if ( qtd_notificacoes < NUM_MAX_NOTICICACOES ){
			var unidade =  MAX_WIDTH_LEGENDAS / NUM_MAX_NOTICICACOES;
			var width = unidade * qtd_notificacoes
			width_legendas = (width < MIN_WIDTH_LEGENDAS)? MIN_WIDTH_LEGENDAS : width;
		}
		table.find('thead ul.legendas').parent().css('max-width',width_legendas);
	});  
});


var PainelNoticiaTurma = (function() {
	return {
		show : function(idNoticia, idTurma){        	
        	BootstrapDialog.show({
        		title: 'Not&#237;cia',
        		message: jQuery('<div><i class="fa fa-spinner fa-pulse"></i> Carregando...</div>').load( '/sigaa/ava/NoticiaTurma/mostrar_modal.jsf?id='+idNoticia+'&tid='+idTurma+'&ajaxRequest' ),
        		buttons: [     
        		    {
                    label: 'Fechar',
                    cssClass: 'btn-primary',
                    action: function(dialog) {
                        dialog.close();
                    }
                }],
            });
        }
	};
})();
</script>
<!-- Fim - Minhas Turmas -->

